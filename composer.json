{
    "name": "drupal/recommended-project",
    "description": "Project template for Drupal 8 projects with a relocated document root",
    "type": "project",
    "license": "GPL-2.0-or-later",
    "homepage": "https://www.drupal.org/project/drupal",
    "support": {
        "docs": "https://www.drupal.org/docs/user_guide/en/index.html",
        "chat": "https://www.drupal.org/node/314178"
    },
    "repositories": [
        {
            "type": "composer",
            "url": "https://packages.drupal.org/8"
        }
    ],
    "require": {
        "acquia/blt": "^11.4",
        "composer/installers": "^1.2",
        "drupal/better_exposed_filters": "^4.0@beta",
        "drupal/core-composer-scaffold": "^8.8",
        "drupal/core-project-message": "^8.8",
        "drupal/core-recommended": "^8.8",
        "drupal/crop": "^2.1",
        "drupal/entity_reference_revisions": "^1.8",
        "drupal/feeds": "^3.0@alpha",
        "drupal/feeds_tamper": "^2.0@beta",
        "drupal/field_group": "^3.0",
        "drupal/fivestar": "^1.0@alpha",
        "drupal/flag": "^4.0@beta",
        "drupal/flag_anon": "^1.0@beta",
        "drupal/fontawesome": "^2.16",
        "drupal/fontyourface": "^3.2",
        "drupal/geofield": "^1.13",
        "drupal/geofield_map": "^2.62",
        "drupal/geolocation": "^3.1",
        "drupal/image_widget_crop": "^2.3",
        "drupal/inline_entity_form": "^1.0@RC",
        "drupal/juicerio": "^1.0@alpha",
        "drupal/mailchimp": "^1.11",
        "drupal/metatag": "^1.13",
        "drupal/onesignal": "1.x-dev",
        "drupal/paragraphs": "^1.11",
        "drupal/simple_gmap": "^2.0",
        "drupal/superfish": "^1.3",
        "drupal/token": "^1.7",
        "drupal/tvi": "^1.0@beta",
        "drupal/video_embed_field": "^2.4",
        "drupal/views_infinite_scroll": "^1.7",
        "drupal/votingapi": "^3.0@beta",
        "drupal/webform": "^5.12"
    },
    "conflict": {
        "drupal/drupal": "*"
    },
    "minimum-stability": "dev",
    "prefer-stable": true,
    "config": {
        "sort-packages": true
    },
    "extra": {
        "drupal-scaffold": {
            "locations": {
                "web-root": "docroot/"
            }
        },
        "installer-paths": {
            "docroot/core": [
                "type:drupal-core"
            ],
            "docroot/libraries/{$name}": [
                "type:drupal-library"
            ],
            "docroot/modules/contrib/{$name}": [
                "type:drupal-module"
            ],
            "docroot/profiles/contrib/{$name}": [
                "type:drupal-profile"
            ],
            "docroot/themes/contrib/{$name}": [
                "type:drupal-theme"
            ],
            "drush/Commands/contrib/{$name}": [
                "type:drupal-drush"
            ],
            "docroot/modules/custom/{$name}": [
                "type:drupal-custom-module"
            ],
            "docroot/themes/custom/{$name}": [
                "type:drupal-custom-theme"
            ]
        },
        "drupal-core-project-message": {
            "include-keys": [
                "homepage",
                "support"
            ]
        }
    }
}
